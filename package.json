    "eslint": "^9.17.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
